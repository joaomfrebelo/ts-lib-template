{
  "name": "ts-lib-template",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist/lib/ && rimraf dist/index.* && tsc && tsc --build tsconfig.es5.json",
    "build:cjs": "rimraf dist/lib/commonjs && tsc --build tsconfig.es5.json",
    "build:es6": "rimraf dist/lib/es6 && rimraf dist/index.* && tsc",
    "parcel": "parcel index.html",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm format && git add -A src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-babel": "^2.8.0",
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "parcel": "^2.8.0",
    "prettier": "^2.8.0",
    "qunit": "^2.19.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "decimal.js-light": "^2.5.1",
    "tsyringe": "^4.7.0"
  }
}
